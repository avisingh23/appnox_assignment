{
  "name": "appnox-assignment",
  "version": "1.0.0",
  "description": "Avnish-Node-TypeScript-Express",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-package && npm run copy-config",
    "build:prod": "npm run build",
    "clean": "shx rm -rf ./dist",
    "copy-package": "shx cp ./package.json ./dist/ && shx cp ./package-lock.json ./dist/",
    "copy-config": "shx cp -R ./src/config ./dist/",
    "live-reload": "nodemon -e js",
    "lint": "eslint . --ext .ts",
    "start": "cd dist && node index.js",
    "dev": "npm run build && tsc --project './tsconfig.json' --watch & NODE_ENV=development cd dist && nodemon",
    "command": "tsc --project './tsconfig.json' & NODE_ENV=development cd dist && node crons/index.js",
    "dev:cluster": "",
    "migration": "migrate-mongo",
    "migration:help": "migrate-mongo",
    "symlink-config": "symlink-dir ./src/config config"
  },
  "author": "avnishtomar87@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/lodash.omit": "^4.5.7",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "eventemitter2": "^6.4.7",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-winston": "^4.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "migrate-mongo": "^9.0.0",
    "mocha": "^9.2.2",
    "mongoose": "^6.2.10",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "symlink-dir": "^5.1.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.10",
    "@swc/helpers": "^0.4.12",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/migrate-mongo": "^8.2.0",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "regenerator-runtime": "^0.13.10",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
